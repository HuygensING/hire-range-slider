{
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "jsx": "react",
		"moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "build",
    "removeComments": true,
    "target": "es6"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/index.tsx"
  ]
}

